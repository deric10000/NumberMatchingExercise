const renderSquare = (index, key) => {
  var squares =
    <button
      className="square"
      index={ index }
      key={ key }
    >{ genRandNum() }</button>;

  const checkSquares = () => {
    return squares.props.index
  }

  console.log(
    checkSquares()
  );

  return squares;
};

const renderSquares = () => {
  var rows = [];
  for (var i = 0; i < 16; i++) {
      rows.push(renderSquare(i,i));
  }
  return rows;
}
